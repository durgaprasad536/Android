<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linear_item"
    android:layout_marginTop="35dp"
    android:background="@color/darkish_blue"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tvHeaderSix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/semibold"
        android:singleLine="false"
        android:layout_centerHorizontal="true"
        android:textColor="@color/white"
        android:textSize="@dimen/text_size_20" />

    <ImageView
        android:id="@+id/imageViewSix"
        android:layout_width="match_parent"
        android:layout_height="235.2dp"
        android:background="@mipmap/onboarding6"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/onboarding_margin_bottom"
        android:layout_marginLeft="58dp"
        android:layout_marginRight="58dp"
          />
</RelativeLayout>
